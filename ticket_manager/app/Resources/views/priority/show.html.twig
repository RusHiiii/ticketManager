{% extends 'layout.html.twig' %}

{% block body %}
    <h1>Priority</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ priority.id }}</td>
            </tr>
            <tr>
                <th>Priorityname</th>
                <td>{{ priority.priorityName }}</td>
            </tr>
            <tr>
                <th>Priorityweight</th>
                <td>{{ priority.priorityWeight }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('priority_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('priority_edit', { 'id': priority.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
